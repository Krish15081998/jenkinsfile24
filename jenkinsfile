pipeline {
    agent { node 'ansible' }
    tools {
        maven 'Maven 3.9.6' 
    }
    stages {
        stage('Git Clone') {
            steps {
                git 'https://github.com/Krish15081998/vk.git'
            }
			}
		stage('Clean') {
		    steps {
			    sh 'mvn clean'
			}
			}	
		stage('List') {
		    steps {
			    sh 'ls -l'
			}
			}
		stage('Validate') {
			steps {
				sh 'mvn validate'
			}
			}
		stage('Compile') {
			steps {
				sh 'mvn compile'
			}
			}
		stage('Test') {
			steps {
				sh 'mvn test'
			}
			}	
		stage('Package') {
			steps {
				sh 'mvn package'
			}
			}	
		stage('Deploy') {
			steps {
				sh 'mvn deploy'
			}
			}	
    }

}

