pipeline {
    agent { node 'ansible' }
    tools {
        maven 'Maven 3.9.6' 
    }
    stages {
        stage('Git Clone') {
            steps {
               git branch: 'master', url: 'https://github.com/Krish15081998/vk.git'
            }
			}
		stage('Clean') {
		    steps {
			    sh 'mvn clean'
			}
			}	
		stage('List') {
		    steps {
			    sh 'ls -l'
			}
			}
		stage('Validate') {
			steps {
				sh 'mvn validate'
			}
			}
		stage('Compile') {
			steps {
				sh 'mvn compile'
			}
			}
		stage('Sonar Scan') {
			steps {
				sh 'mvn sonar:sonar -Dsonar.host.url=http://34.125.241.73:9000 -Dsonar.login=d90cf9aeca730cb5531aa7ce4e1e37768b7fbe88'
			}
			}	
		stage('Test') {
			steps {
				sh 'mvn test'
			}
			}	
		stage('Package') {
			steps {
				sh 'mvn package'
			}
			}	
		stage('Deploy') {
			steps {
				sh 'mvn deploy'
			}
			}	
    }

}

